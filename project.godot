; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cue2"
config/version="0.00.003"
run/main_scene="res://src/Base/cue_2_base.tscn"
config/features=PackedStringArray("4.3", "C#")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 0)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

GlobalSignals="*res://src/Shared/GlobalSignals.cs"
SaveManager="*res://src/Shared/SaveManager.cs"
GlobalData="*res://src/Shared/GlobalData.cs"
Hardware="res://src/Shared/Hardware.cs"
Connections="*res://src/Shared/Connections.cs"
GlobalStyles="*res://src/Shared/GlobalStyles.cs"
ErrorHandler="*res://src/Shared/Error_Handler.cs"

[display]

window/size/borderless=true
window/subwindows/embed_subwindows=false
window/per_pixel_transparency/allowed=true
window/handheld/orientation=1
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Cue2"

[editor]

naming/default_signal_callback_name="_on{NodeName}{SignalName}"
naming/default_signal_callback_to_self_name="_on{Signal_name}"
naming/scene_name_casing=1
import/reimport_missing_imported_files=false

[editor_plugins]

enabled=PackedStringArray()

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0, 0, 0, 0)
